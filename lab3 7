#include <iostream>
using namespace std;

int main() {

              char x;
              cout<<"enter character";
              cin>>x;
              
              if((x>='a' && x<='z') || (x>='A' && x<='Z'))
              {
                 cout<<"it is an alphabet",x;
              }
              else
              {
                 cout<<"it is not alpabet",x;
              }
            
	return 0;
}
