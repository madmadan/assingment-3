#include <iostream>
using namespace std;

int main() {

              int num;
              cout<<"enter number";
              cin>>num;
              
              if((num%5 == 0.0)&&(num%11 == 0.0))
              {
                 cout<<"number is divisible by 5 and 11";
              }
              else
              {
                 cout<<"number isn't divisible by 5 and 11";
              }
            
	return 0;
}
