#include<iostream>
using namespace std;

int main()
          {
	int a,b,c;

             cout<<"Enter three sides of triangle: \n";
             cin>>a>>b>>c;

             if(c==a&&a==b&&b==c) 
             {
                     cout<<"equilateral Triangle.";
             }
             else if(a==b||b==c||a==c)
             {
                     cout<<"isoceles Triangle.";
             }
             else
             {
                     cout<<"scalene Triangle.";
             }

	
	return 0;
}
